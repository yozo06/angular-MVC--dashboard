 wip:-
 SELECT eof.HOSTNAME, eof.USERNAME, re.REPO_CMD_CODE, 
           eof.EVENT_TIMESTAMP
           FROM EVENTS_ON_FILE eof, REPO_EVENTS re
           WHERE eof.REPO_CMD_ID = re.REPO_CMD_ID
		   AND eof.EVENT_ID IN
             ( SELECT max(ef.EVENT_ID) AS EVENTS
               FROM FILE_DETAILS fd, EVENTS_ON_FILE ef, BRANCH_DETAILS bd
               WHERE fd.FILE_ID = ef.FILE_ID
               AND   ef.BRANCH_ID = bd.BRANCH_ID
               GROUP BY fd.FILE_ID,  ef.HOSTNAME, ef.USERNAME)
			AND (re.REPO_CMD_CODE != 'SVNCOMMIT' OR re.REPO_CMD_CODE != 'GITPUSH');
      
            
others:-
select COUNT(*) from FILE_DETAILS;
select BRANCHNAME from BRANCH_DETAILS;
select RELEASE_NAME from BRANCH_DETAILS;
SELECT * FROM EVENTS_ON_FILE WHERE FILE_ID=29;
SELECT COUNT(DISTINCT FILE_ID) FROM EVENTS_ON_FILE WHERE BRANCH_ID=1;

select RELEASE_NAME from BRANCH_DETAILS WHERE BRANCH_ID=1;


CREATE VIEW MULTIPLE_USERS_REF AS SELECT COUNT(USERNAME) AS NUM_OF_USERS , FILE_ID FROM EVENTS_ON_FILE GROUP BY FILE_ID;

SELECT COUNT(NUM_OF_USERS) FROM MULTIPLE_USERS_REF  WHERE NUM_OF_USERS>1;

CREATE OR REPLACE VIEW MULTIPLE_USERS_REF AS SELECT COUNT(USERNAME) AS NUM_OF_USERS , FILE_ID FROM EVENTS_ON_FILE WHERE BRANCH_ID=1 GROUP BY FILE_ID;

SELECT NUM_OF_USERS,FILE_NAME FROM MULTIPLE_USERS_REF NATURAL JOIN FILE_DETAILS HAVING NUM_OF_USERS>1;
